{
  "name": "nodejs-resolver",
  "version": "0.0.2",
  "description": "Node binding for nodejs-resolver(rs)",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --dts index.d.ts",
    "build:release": "napi build --release --platform --dts index.d.ts",
    "test": "node test.js"
  },
  "author": "bohan-zhang@foxmail.com",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "2.9.0"
  },
  "napi": {
    "name": "nodejs-resolver",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  }
}
